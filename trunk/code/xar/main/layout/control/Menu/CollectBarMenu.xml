		<attr>
			<left>0</left>
			<top>0</top>
			<width>260</width>
			<height>278</height>
		</attr>
		
		<obj id="Menu.MainLayout" class="LayoutObject">
			<attr>
				<left>0</left>
				<top>0</top>
				<width>260</width>
				<height>278</height>
			</attr>
			<children>
				<obj id="Menu.Context" class="NormalMenu">
					<attr>
						<left>0</left>
						<top>0</top>
						<width>260</width>
						<height>father.height</height>
						<!--ContextID>collectmenu.context</ContextID-->
						<BknID>YBYL.Menu.Bkg</BknID>
					</attr>
					<eventlist>
						<event name="OnInitControl" file="CollectBarMenu.xml.lua" func="OnInitControl" />
					</eventlist>
				</obj>
			</children>
		</obj>
	</objtreetemplate>
</xlue>
