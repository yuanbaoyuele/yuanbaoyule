			<obj id="spilt2" class="MenuItemObject" templateid="menu.splitter">
				<attr>
				</attr>
			</obj>
			
			<obj id="CreateShortCut" class="MenuItemObject" templateid="menu.context.item">
				<attr>
					<Text>发送到桌面快捷方式</Text>
				</attr>
				<eventlist>
					<event name="OnSelect" file="FileMenu.xml.lua" function="OnSelect_ShortCut" />
				</eventlist>
