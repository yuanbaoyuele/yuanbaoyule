<xlue>
	<control class="ToolTipCtrl">
		<attr_def>
			
		</attr_def>
		<method_def>
			<SetToolTipText file="ToolTip.xml.lua" func="SetToolTipText"/>
			<GetToolTipWidth file="ToolTip.xml.lua" func="GetToolTipWidth"/>
			<AdjustTextPos file="ToolTip.xml.lua" func="AdjustTextPos"/>
		</method_def>
		<event_def>
			
		</event_def>
		<objtemplate>
			<children>
				<obj id="ToolTipLayout" class="TextureObject">
					<attr>
						<left>0</left>
						<top>0</top>
						<width>father.width</width>
						<height>father.height</height>
						<texture>XmpTipWnd.ToopTip.Bkg</texture>
					</attr>
					<children>
						<obj id="ToolTipText" class="TextObject">
							<attr>
								<left>0</left>
								<top>0</top>
								<width>father.width</width>
								<height>father.height</height>
								<font>font.yahei13</font>
								<textcolor>movietime.black</textcolor>
							</attr>
						</obj>
					</children>
				</obj>						
			</children>
		</objtemplate>
	</control>
	 
	<objtreetemplate id="ToolTipTree" class="ObjectTreeTemplate">
		<attr>
			<left>-200</left>
			<top>-200</top>
			<width>154</width>
			<height>28</height>
		</attr>

		<obj id="RootCtrl" class="ToolTipCtrl">
		    <attr>
			    <left>0</left>
				<top>0</top>
				<width>154</width>
				<height>28</height>
			</attr>
			<eventlist>
				<event name="OnMouseEnter" file="ToolTip.xml.lua" func="OnMouseEnter_Root" />
			</eventlist>
		</obj>
	</objtreetemplate>
	
	
	<hostwndtemplate id="ToolTipWnd" class="TipsHostWnd" >
		<attr>
			<layered>1</layered>
			<left>200</left>
			<top>100</top>
			<width>200</width>
			<height>50</height>
			<topmost>1</topmost>
			<visible>1</visible>
			<fps>30</fps>
		</attr>
	</hostwndtemplate>
</xlue>