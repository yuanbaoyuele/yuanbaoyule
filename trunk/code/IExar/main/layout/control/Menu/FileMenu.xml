					<Text>关闭选项卡</Text>
					<AccKey>C</AccKey>
					<RightText>Ctrl+W</RightText>
				</attr>
				<eventlist>
					<event name="OnSelect" file="FileMenu.xml.lua" function="OnSelect_CloseTab" />
				</eventlist>
