				<eventlist>
					<event name="OnSelect" file="FileMenu.xml.lua" function="OnSelect_CopyTab" />
				</eventlist>
			</obj>
			
			<obj id="AddNewWindow" class="MenuItemObject" templateid="menu.context.item">
