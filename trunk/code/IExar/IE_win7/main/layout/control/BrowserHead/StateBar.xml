							</eventlist>
						</obj>
						
						
						<obj id="StateBar.RightCorner" class="ImageObject">
							<attr>
								<left>father.width-11</left>
								<top>father.height-11</top>
								<width>11</width>
								<height>11</height>
								<image>StateBar.RightCorner</image>
							</attr>
						</obj>
						
