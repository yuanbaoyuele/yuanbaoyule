											<event name="OnMouseLeave" file="StateBar.xml.lua" func="RouteToFather" />
										</eventlist>
									</obj>
								</children>
							<eventlist>
								<event name="OnClick" file="StateBar.xml.lua" func="OnClickZoomText" />
